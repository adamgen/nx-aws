{
  "name": "@nx-aws/sam",
  "version": "0.3.3",
  "description": "AWS SAM plugin for nx",
  "keywords": [
    "AWS",
    "SAM",
    "Serverless",
    "Angular CLI",
    "Workspace",
    "Nx",
    "Monorepo"
  ],
  "main": "dist/sam.js",
  "builders": "builders.json",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -Rf dist",
    "build:ts": "tsc --build packages/sam/tsconfig.json",
    "build:mpt": "monorepo-package-tool",
    "build:cp": "cp packages/sam/builders.json dist/packages/sam",
    "build:rsync": "cd packages/sam/src; rsync -R **/*.json ../../../dist/packages/sam; cd ../../..",
    "build": "run-s build:*",
    "prepublishOnly": "run-s clean build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.616.0",
    "cloudform-types": "^4.1.0",
    "cloudformation-js-yaml-schema": "^0.4.2",
    "constant-case": "^3.0.3",
    "js-yaml": "^3.13.1",
    "mkdirp": "^1.0.3",
    "source-map-support": "^0.5.13",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">= 0.900.0",
    "@angular-devkit/build-webpack": ">= 0.900.0",
    "@angular-devkit/core": ">= 9.0.0",
    "@nrwl/node": ">= 8.2.0",
    "@nrwl/workspace": ">= 8.2.0",
    "rxjs": ">= 6.4.0",
    "webpack": ">= 4.41.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.1",
    "@angular-devkit/build-webpack": "^0.900.1",
    "@angular-devkit/core": "^9.0.1",
    "@nrwl/node": "^8.12.4",
    "@nrwl/workspace": "^8.12.4",
    "@types/js-yaml": "^3.12.2",
    "@types/mkdirp": "^1.0.0",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-merge": "^4.1.5",
    "monorepo-package-tool": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "^6.5.4",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  },
  "repository": "https://github.com/studds/nx-aws"
}
