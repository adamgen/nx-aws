{
  "name": "nx-aws",
  "version": "0.9.0",
  "description": "AWS plugins for nx",
  "keywords": [
    "AWS",
    "SAM",
    "Serverless",
    "Angular CLI",
    "Workspace",
    "Nx",
    "Monorepo",
    "s3"
  ],
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --all",
