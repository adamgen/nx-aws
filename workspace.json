                        "tsConfig": "packages/s3/tsconfig.lib.json",
                        "packageJson": "packages/s3/package.json",
                        "main": "packages/s3/src/index.ts",
                        "assets": [
                            "packages/s3/*.md",
                            {
                                "input": "./packages/s3/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
