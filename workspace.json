                    "options": {
                        "jestConfig": "packages/s3/jest.config.js",
                        "tsConfig": "packages/s3/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
