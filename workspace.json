                            }
                        ]
                    }
                },
                "publish": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npx monorepo-package-tool --rootModuleDir packages/s3 --destDir dist/packages/s3"
                            },
                            {
                                "command": "npm publish dist/packages/s3 --access public"
                            }
                        ]
                    }
                },
                "pack": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
