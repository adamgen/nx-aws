{
    "version": 1,
    "projects": {
        "sam": {
            "root": "packages/sam",
            "sourceRoot": "packages/sam/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "packages/sam/tsconfig.lib.json",
                            "packages/sam/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!packages/sam/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/sam/jest.config.js",
                        "tsConfig": "packages/sam/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/sam",
                        "tsConfig": "packages/sam/tsconfig.lib.json",
                        "packageJson": "packages/sam/package.json",
                        "main": "packages/sam/src/index.ts",
                        "assets": [
                            "packages/sam/*.md",
                            {
                                "input": "./packages/sam/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/sam",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
                                "input": "./packages/sam",
                                "glob": "builders.json",
                                "output": "."
                            }
                        ]
                    }
                },
                "publish": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npx monorepo-package-tool --rootModuleDir packages/sam --destDir dist/packages/sam"
                            },
                            {
                                "command": "npm publish dist/packages/sam"
                            }
                        ]
                    }
                },
                "pack": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npx monorepo-package-tool --rootModuleDir packages/sam --destDir dist/packages/sam"
                            },
                            {
                                "command": "npm pack dist/packages/sam"
                            }
                        ]
                    }
                }
            }
        },
        "sam-e2e": {
            "projectType": "application",
            "root": "e2e/sam-e2e",
            "sourceRoot": "e2e/sam-e2e/src",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/nx-plugin:e2e",
                    "options": {
                        "target": "sam:build",
                        "npmPackageName": "@nx-aws/sam",
                        "pluginOutputPath": "dist/packages/sam",
                        "jestConfig": "e2e/sam-e2e/jest.config.js",
                        "tsSpecConfig": "e2e/sam-e2e/tsconfig.spec.json"
                    }
                }
            }
        },
        "core": {
            "root": "packages/core",
            "sourceRoot": "packages/core/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "packages/core/tsconfig.lib.json",
                            "packages/core/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!packages/core/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/core/jest.config.js",
                        "tsConfig": "packages/core/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/core",
                        "tsConfig": "packages/core/tsconfig.lib.json",
                        "packageJson": "packages/core/package.json",
                        "main": "packages/core/src/index.ts",
                        "assets": [
                            "packages/core/*.md",
                            {
                                "input": "./packages/core/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/core",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
                                "input": "./packages/core",
                                "glob": "builders.json",
                                "output": "."
                            }
                        ]
                    }
                },
                "publish": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npx monorepo-package-tool --rootModuleDir packages/core --destDir dist/packages/core"
                            },
                            {
                                "command": "npm publish dist/packages/core --access public"
                            }
                        ]
                    }
                },
                "pack": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npx monorepo-package-tool --rootModuleDir packages/core --destDir dist/packages/core"
                            },
                            {
                                "command": "npm pack dist/packages/core"
                            }
                        ]
                    }
                }
            }
        },
        "core-e2e": {
            "projectType": "application",
            "root": "e2e/core-e2e",
            "sourceRoot": "e2e/core-e2e/src",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/nx-plugin:e2e",
                    "options": {
                        "target": "core:build",
                        "npmPackageName": "@nx-aws/core",
                        "pluginOutputPath": "dist/packages/core",
                        "jestConfig": "e2e/core-e2e/jest.config.js",
                        "tsSpecConfig": "e2e/core-e2e/tsconfig.spec.json"
                    }
                }
            }
        },
        "s3": {
