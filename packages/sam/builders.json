{
    "$schema": "@angular-devkit/architect/src/builders-schema.json",
    "builders": {
        "build": {
            "implementation": "./build/build",
            "schema": "./build/schema.json",
            "description": "Build a SAM application"
        },
        "execute": {
            "implementation": "./execute/execute",
            "schema": "./execute/schema.json",
            "description": "Execute a SAM application"
        },
        "package": {
            "implementation": "./cloudformation/package/package",
            "schema": "./cloudformation/package/schema.json",
            "description": "Package a SAM application"
        },
        "deploy": {
            "implementation": "./cloudformation/deploy/deploy",
            "schema": "./cloudformation/deploy/schema.json",
            "description": "Deploy a SAM application"
        }
    }
}
